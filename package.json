{
  "name": "mati-arbio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview --host",
    "ar:compile": "npx mind-ar-tools@latest compile-image --input public/ar-demo/images/pitcher.jpg --output public/ar-demo/targets.mind --filter 35 --debug",
    "optimize": "npm run build && npm run preview",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --max-warnings=0 || true",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "firebase": "^10.12.0",
    "fuse.js": "^7.1.0",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.10",
    "terser": "^5.44.0",
    "typescript": "^5.5.4",
    "vite": "^5.3.1",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^1.1.5"
  }
}
